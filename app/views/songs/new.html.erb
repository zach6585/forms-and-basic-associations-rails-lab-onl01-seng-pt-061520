<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <input id="song_notes_1" name="song[note_vals][]">
  <input id="song_notes_2" name="song[note_vals][]">
  
  <%= f.submit %>
<% end %>

